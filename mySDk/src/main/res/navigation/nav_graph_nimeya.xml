        <action
            android:id="@+id/action_nimeyaHomeFragment_to_riskoMeterInputFragment"
            app:destination="@id/riskoMeterInputFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
