            app:popExitAnim="@anim/slide_out_right"/>

        <action
            android:id="@+id/action_nimeyaHomeFragment_to_riskoMeterResultFragment"
            app:destination="@id/riskoMeterResultFragment"
            app:enterAnim="@anim/slide_in_right"
