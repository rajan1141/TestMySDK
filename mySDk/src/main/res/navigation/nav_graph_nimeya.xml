<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_nimeya"
    app:startDestination="@id/nimeyaHomeFragment">

    <fragment
        android:id="@+id/nimeyaHomeFragment"
        android:name="com.test.my.app.home.ui.nimeya.NimeyaHomeFragment"
