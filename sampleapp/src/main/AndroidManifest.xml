<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".App"
        android:allowBackup="false"
        tools:ignore="DataExtractionRules,GoogleAppIndexingWarning"
        tools:replace="android:allowBackup"
        android:icon="@drawable/app_logo_white"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/app_logo_white"
        android:supportsRtl="true"
        android:theme="@style/Theme.Youmatter_android_SDK_New"
        >

        <activity
            android:name=".SampleMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>